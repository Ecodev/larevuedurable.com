# This is a template .gitignore file for git-managed Prestashop projects. Inspired by micahwalter wordpress gitignore
 
#Ignore ds_store
*.DS_Store

# Ignore everything in the root except modules, themes and override.
/*
!.gitignore

!bin/

!htdocs/
htdocs/*
htdocs/.htaccess

!htdocs/config/
htdocs/config/*
!htdocs/config/defines.inc.php
!htdocs/config/smartyadmin.config.inc.php
!htdocs/config/settings.empty.inc.php

!htdocs/classes/
htdocs/classes/*
!htdocs/classes/Subscription.php

!htdocs/modules/
htdocs/modules/ecodevscripts/de_cresus/*
htdocs/modules/ecocevscripts/pour_cresus/*
htdocs/modules/ecodevscripts/tests/*
htdocs/modules/prestablog/themes/*/up-img/*
!htdocs/override/

!htdocs/manuel/
htdocs/manuel/sources/videos/*

!htdocs/themes/
htdocs/themes/*
!htdocs/themes/larevuedurable/
htdocs/themes/larevuedurable/cache/*.js
htdocs/themes/larevuedurable/cache/*.css
htdocs/themes/larevuedurable/modules/*/translations/*
